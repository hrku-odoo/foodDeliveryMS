<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="menu_tag_view_tree" model="ir.ui.view">
        <field name="name">food.delivery.menu.tag.tree</field>
        <field name="model">food.delivery.menu.tag</field>
        <field name="arch" type="xml">
            <tree string="Menu Tags" >
                <field name="name"/>
            </tree>
        </field>
    </record>


    <record id="food_delivery_menu_tag_action" model="ir.actions.act_window">
    <field name="name">Menu Tag</field>
    <field name="res_model">food.delivery.menu.tag</field>
    <field name="view_mode">tree,form</field>
    </record>
</odoo>