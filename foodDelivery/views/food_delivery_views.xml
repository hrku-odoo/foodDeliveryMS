<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <record id="food_delivery_view_tree" model="ir.ui.view">
        <field name="name"></field>
        <field name="model">food.delivery</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="address" />
                <field name="postcode" />
                <field name="cuisine_type" />
            </tree>
        </field>
    </record>

    <record id="food_delivery_action" model="ir.actions.act_window">
        <field name="name">Restaurants</field>
        <field name="res_model" >food.delivery</field>
        <field name="view_mode">tree,form</field>
        <!-- <field name="context">{'search_default_available': 1}</field> -->
    </record>

</odoo>